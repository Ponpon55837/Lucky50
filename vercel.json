{
  "rewrites": [
    {
      "source": "/Dashboard",
      "destination": "/dashboard"
    },
    {
      "source": "/Profile",
      "destination": "/profile"
    },
    {
      "source": "/Analytics",
      "destination": "/analytics"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    }
  ]
}